<UserControl x:Class="ThicknessAndComposition_Inspector_IPS.UC_MapInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ThicknessAndComposition_Inspector_IPS"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200">
    <UniformGrid Rows="8" Columns="1" Background="Aquamarine">
        
        <DockPanel>
            <Rectangle Fill="AliceBlue" Style="{StaticResource rectRside}"/>
            <Label Content="100~200" Style="{StaticResource lblRSide}"/>
        </DockPanel>

        <DockPanel>
            <Rectangle Fill="AliceBlue" Style="{StaticResource rectRside}"/>
            <Label Content="100~200" Style="{StaticResource lblRSide}"/>
        </DockPanel>

    </UniformGrid>
</UserControl>
